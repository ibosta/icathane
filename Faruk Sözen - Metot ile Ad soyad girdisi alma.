using System;

class Odev
{
    static void AdSoyadYaz(string ad, string soyad)
    {
        Console.WriteLine("Ad Soyad: " + ad + " " + soyad);
    }

    static void Main()
    {
        Console.Write("Adiniz: ");
        string ad = Console.ReadLine();

        Console.Write("Soyadiniz: ");
        string soyad = Console.ReadLine();

        AdSoyadYaz(ad, soyad);
    }
}
