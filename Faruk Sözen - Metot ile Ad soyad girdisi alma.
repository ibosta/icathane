using System; 

class ödev
{
    static string AdSoyad(string ad, string soyad)
    {
        return ad + " " + soyad;
    } 

    static void Main(string[] args)
    {
        Console.WriteLine("Adiniz: ");
        string ad = Console.ReadLine(); 

        Console.WriteLine("Soyadiniz: ");
        string soyad = Console.ReadLine(); 

        string sonuc = AdSoyad(ad, soyad);
        Console.WriteLine("Ad Soyad: " + sonuc);
    }
}
