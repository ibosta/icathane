
using System;

public class HelloWorld
{
    public static void Main(string[] args)
    {
       int i = 0 ;
       int j = 0 ;
       while(i < 3)
       {
           j = 0;
           while(j < 3)
           {
               Console.Write("*");
               j++;
           }
           Console.WriteLine();
           i++ ;
       }
    }
}
